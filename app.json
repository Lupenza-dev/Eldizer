{
  "expo": {
    "name": "Chuo Credit",
    "slug": "chuo-credit",
    "version": "1.7.0",
    "orientation": "portrait",
    "icon": "./assets/loogo_2.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/framew.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.lupenza.Eldizer",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "compileSdkVersion": 35,
      "targetSdkVersion": 35,
      "adaptiveIcon": {
        "foregroundImage": "./assets/loogo_2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.lupenza.Eldizer",
      "googleServicesFile": "./google-services.json",
      "permissions": [],
      "versionCode": 6,
      "blockedPermissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_VIDEOS",
        "android.permission.READ_MEDIA_VIDEO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "0030eed7-679c-408a-96d4-cefd29ecd468"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/fe51a432-8598-4fd9-bc79-bea4e25bc393"
    },
    "plugins": [
      [
        "expo-updates",
        {
          "username": "lupenza"
        }
      ],
      
      "expo-font",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",   // already default in SDK 52, but safe to keep
            "modularHeaders": true       // ‚Üê this fixes your GoogleUtilities error
          },
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0",
            "nativePageSizes": ["16KB"]
          }
        }
      ]
    ]
  }
}
